
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no" name="viewport">
    <title>@ViewBag.Title</title>
    <!-- General CSS Files -->
    <link rel="stylesheet" href="../AssociateCSS/assets/css/app.min.css">
    <!-- Template CSS -->
    <link rel="stylesheet" href="../AssociateCSS/assets/bundles/select2/dist/css/select2.min.css">
    <link rel="stylesheet" href="../AssociateCSS/assets/bundles/jquery-selectric/selectric.css">
    <link rel="stylesheet" href="../AssociateCSS/assets/bundles/datatables/datatables.min.css">
    <link rel="stylesheet" href="../AssociateCSS/assets/bundles/datatables/DataTables-1.10.16/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="../AssociateCSS/assets/css/style.css">
    <link rel="stylesheet" href="../AssociateCSS/assets/css/components.css">
    <link rel="stylesheet" href="../AssociateCSS/assets/bundles/jqvmap/dist/jqvmap.min.css">
    @*<link href="http://localhost:23917/WebsiteCSS/select2/dist/css/select2.min.css" rel="stylesheet" />*@
    <!-- Custom style CSS -->
    <link rel="stylesheet" href="../AssociateCSS/assets/css/custom.css">
    <link rel='shortcut icon' type='image/x-icon' href="~/WebsiteCss/assets/images/favicon.png" />
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <style>
        .errortext {
            border: 1px solid red !important;
            background-color: #fff0f0 !important;
        }
    </style>
</head>

<body>
    <div class="loader" id="divload"></div>
    <div id="app">
        <div class="main-wrapper main-wrapper-1">
            <div class="navbar-bg"></div>
            <nav class="navbar navbar-expand-lg main-navbar sticky">
                <div class="form-inline mr-auto">
                    <ul class="navbar-nav mr-3">
                        <li>
                            <a href="#" data-toggle="sidebar" class="nav-link nav-link-lg
									collapse-btn"><i data-feather="menu"></i></a>
                        </li>
                        <li>
                            @*<form class="form-inline mr-auto">
                                    <div class="search-element">
                                        <input class="form-control" type="search" placeholder="Search" aria-label="Search" data-width="200">
                                        <button class="btn" type="submit">
                                            <i class="fas fa-search"></i>
                                        </button>
                                    </div>
                                </form>*@
                        </li>
                    </ul>
                </div>
                <ul class="navbar-nav navbar-right">
                    <li>
                        <a href="#" class="nav-link nav-link-lg fullscreen-btn">
                            <i data-feather="maximize"></i>
                        </a>
                    </li>

                    <li class="dropdown">
                        <a href="#" data-toggle="dropdown"
                           class="nav-link dropdown-toggle nav-link-lg nav-link-user">

                            <img alt="image" src="~/AssociatePanelcss/assets/img/icon.png"
                                 class="user-img-radious-style"> <span class="d-sm-none d-lg-inline-block"></span>
                        </a>
                        <div class="dropdown-menu dropdown-menu-right pullDown">
                            <div class="dropdown-title">Hello @Session["LoginId"]</div>


                            <div class="dropdown-divider"></div>
                            <a href="/Home/Login" class="dropdown-item has-icon text-danger">
                                <i class="fas fa-sign-out-alt"></i>
                                Logout
                            </a>
                        </div>
                    </li>
                </ul>
            </nav>
            <div class="main-sidebar sidebar-style-2">
                <aside id="sidebar-wrapper">
                    <div class="sidebar-brand">
                        <a href="~/Admin/AdminDashBoard">
                            <img class="header-logo" alt="@Session["Name"]" src="~/WebsiteCss/assets/images/radhekunj_logo.png" title="@Session["Name"]" />
                            @*<span class="logo-name text-white">Radhe Kunj</span>*@
                        </a>
                    </div>
                    <div class="sidebar-user">
                        <div class="sidebar-user-picture">
                            <img alt="image" src="~/AssociatePanelcss/assets/img/icon.png">

                        </div>
                        <div class="sidebar-user-details">
                            <div class="user-name">Welcome -: @Session["Name"]</div>
                            @*  <div class="user-role">Administrator</div>*@
                        </div>
                    </div>

                </aside>
                @{Html.RenderAction("Menu", "Home", new { area = "" });}
            </div>

            <div class="main-content">
                <section class="section">
                    @RenderBody()
                </section>
            </div>
            <footer class="main-footer">
                <div class="footer-left">
                    Copyright &copy; 2020 <div class="bullet"></div> Design By <a href="#">Radhe Kunj Infratech pvt. ltd. LLP</a>
                </div>
                <div class="footer-right">
                </div>
            </footer>
        </div>
    </div>
    <!-- General JS Scripts -->
    <script src="../AssociateCSS/assets/js/app.min.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <!-- JS Libraies -->
    <!-- Page Specific JS File -->
    <script src="../AssociateCSS/assets/bundles/select2/dist/js/select2.full.min.js"></script>
    <script src="../AssociateCSS/assets/bundles/jquery-selectric/jquery.selectric.min.js"></script>
    <script src="../AssociateCSS/assets/bundles/datatables/datatables.min.js"></script>
    <script src="../AssociateCSS/assets/bundles/datatables/DataTables-1.10.16/js/dataTables.bootstrap4.min.js"></script>
    <script src="../AssociateCSS/assets/bundles/datatables/export-tables/dataTables.buttons.min.js"></script>
    <script src="../AssociateCSS/assets/bundles/datatables/export-tables/buttons.flash.min.js"></script>
    <script src="../AssociateCSS/assets/bundles/datatables/export-tables/jszip.min.js"></script>
    <script src="../AssociateCSS/assets/bundles/datatables/export-tables/pdfmake.min.js"></script>
    <script src="../AssociateCSS/assets/bundles/datatables/export-tables/vfs_fonts.js"></script>
    <script src="../AssociateCSS/assets/bundles/datatables/export-tables/buttons.print.min.js"></script>
    <script src="../AssociateCSS/assets/js/page/datatables.js"></script>
    <!-- Template JS File -->
    <script src="~/AssociateCSS/assets/js/scripts.js"></script>
    <script src="~/AssociateCSS/assets/bundles/izitoast/js/iziToast.min.js"></script>
    <script src="~/AssociateCSS/assets/bundles/sweetalert/sweetalert.min.js"></script>
    <!-- Custom JS File -->
    <script src="~/AssociateCSS/assets/js/custom.js"></script>
    <!-- General JS Scripts -->
    <!-- JS Libraies -->
    <script src="../AssociateCSS/assets/bundles/apexcharts/apexcharts.min.js"></script>
    <script src="../AssociateCSS/assets/bundles/jqvmap/dist/jquery.vmap.min.js"></script>
    <script src="../AssociateCSS/assets/bundles/jqvmap/dist/maps/jquery.vmap.world.js"></script>
    <!-- Page Specific JS File -->
    <script src="../AssociateCSS/assets/js/page/index.js"></script>
    <!-- Template JS File -->
    <script src="../AssociateCSS/assets/js/scripts.js"></script>
    <!-- Custom JS File -->
    <script src="../AssociateCSS/assets/js/custom.js"></script>

    <!-- Page Specific JS File -->
    <script src="../AssociateCSS/assets/js/page/chart-apexcharts.js"></script>
    <!-- Template JS File -->
    <script src="../AssociateCSS/assets/js/scripts.js"></script>

    <!-- Custom JS File -->
    @RenderSection("script", required: false)
    
</body>

</html>




